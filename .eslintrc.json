{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:html/recommended",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "html"
    ],
    "rules": {
        "no-console": "warn",
        "no-unused-vars": "warn",
        "prefer-const": "error",
        "no-var": "error"
    }
}